{
  "name": "play-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile:circuits && npm run setup && npm run compile",
    "compile": "hardhat compile && typechain --out-dir types --target ethers-v5 './artifacts/contracts/**/!(*.dbg).json'",
    "compile:circuits": "mkdir -p build && circom circuits/withdraw.circom --r1cs --wasm -o build",
    "setup": "sh quickSetup.sh",
    "info": "snarkjs info -r build/withdraw.r1cs",
    "test": "hardhat test tests/*",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@axelar-network/axelar-gmp-sdk-solidity": "^3.3.0",
    "@ethersproject/solidity": "^5.7.0",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.3",
    "@typechain/ethers-v5": "^11.1.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.11.25",
    "chai": "^4.3.6",
    "chalk": "4.1.2",
    "circom": "^0.5.46",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "dotenv": "^16.3.1",
    "ethers": "^5.5.4",
    "fixed-merkle-tree": "^0.7.3",
    "hardhat": "^2.11.1",
    "mocha": "^10.0.0",
    "prettier": "2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "snarkjs": "^0.7.0",
    "ts-node": "^10.5.0",
    "typechain": "^8.3.1",
    "typescript": "^4.5.5",
    "vmtree-sdk": "vmtree/sdk#d2b1b91c24ec8a4e70058f9f2b0db0b9651ae948"
  },
